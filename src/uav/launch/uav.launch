<launch>
	<node pkg="uav" type="uav" name="uav" output="screen">
	<param name="debug"               type="bool"   value="true"/>
	<param name="enable_step"         type="bool"   value="true"/>
	<param name="spin_rate"           type="int"    value="50"/>
	<param name="serial_port"         type="string" value="/dev/ttyTHS2"/>  
  	<param name="serial_baudrate"     type="int"    value="115200"/>
	<param name="open_claw_cmd"       type="int"  	value="0"/>
	<param name="close_claw_cmd"      type="int"  value="1"/>
	<param name="takeoff_height"      type="double"  value="1.0"/>
	<param name="landing_height"      type="double"  value="0.3"/>
	<param name="xy_err_tolerence"    type="double"  value="0.1"/>
	<param name="z_err_tolerence"     type="double"  value="0.1"/>
	<param name="yaw_err_tolerence"   type="double"  value="0.1"/>
	<param name="vision_pos_coeff"    type="double"  value="0.001"/>
	<param name="serial_timeout"      type="int"  value="400"/>
	<param name="callback_timeout"    type="int"  value="400"/>
	<rosparam command="load" file="$(find uav)/config/pid.yaml" />
	<rosparam command="load" file="$(find uav)/config/dropoint.yaml" />
	<remap from="">
	</node>
</launch>
